<odoo>

<record id="fleet_vehicle_tranpoter_views" model="ir.ui.view">
		<field name="name">fleet.vehicle.tranpoter.views</field>
		<field name="model">fleet.vehicle</field>
		<field name="inherit_id" ref="fleet.fleet_vehicle_view_form"/>
