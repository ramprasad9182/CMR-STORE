                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="description_6" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="description_7" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
