<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="stock_picking_view_for_last_serial_number" model="ir.ui.view">
        <field name="name">view.picking.form.inherit.last.serial</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[1]" position="after">
                <group string="Transport Details" invisible="stock_picking_type not in ['regular','damage','return','receipt']">
                    <group>
                        <field name="transpoter_id" readonly="state == 'done'" required="stock_picking_type  in ['regular','damage']" options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                        <field name="transpoter_route_id" readonly="state == 'done'" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                               domain="[('transpoter_id','=',transpoter_id)]" required="stock_picking_type  in ['regular','damage']"/>
                        <field name="lr_number" readonly="state == 'done'"/>
                        <field name="driver_name" readonly="state == 'done'" required="stock_picking_type  in ['regular','damage']"/>
                    </group>
                    <group>
                        <field name="no_of_parcel" readonly="state == 'done'" required="stock_picking_type  in ['regular','damage']"/>
                        <field name="tracking_number" readonly="state == 'done'"/>
                        <field name="nhcl_tracking_number" readonly="1" force_save="1"/>
                        <field name="vehicle_number" readonly="state == 'done'" required="stock_picking_type  in ['regular','damage']"/>
                    </group>
                </group>
            </xpath>
            <xpath expr="//page[@name='note']" position="after">
                <page string="Transport Routes" name="tranport_routes" invisible="stock_picking_type not in ['regular','damage','return','receipt']">
                    <field name="transport_location_line" readonly="1">
                        <tree create='false'>
                            <field name="source_location_id"/>
                            <field name="destination_location_id"/>
                            <field name="distance"/>
                            <field name="transport_charges"/>
                            <field name="time_hour"/>
                            <field name="start_time"/>
                            <field name="end_time"/>
                            <field name="tracking_number"/>
                            <field name="state"/>
                        </tree>
                    </field>
                </page>
                <page string="Transport Entry" invisible="stock_picking_type not in ['regular','damage','return','receipt']">
                    <field name="transport_entry_ids" readonly="1"/>
                </page>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/tree" position="attributes">
                <attribute name="delete">false</attribute>
                <attribute name="create">false</attribute>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/tree" position="inside">
                <button name="picking_unlink" type="object" class="fa fa-trash" invisible="parent.picking_type_code != 'internal'"/>
            </xpath>
            <xpath expr="//field[@name='partner_id']" position="attributes">
                <attribute name="options">{'no_create': True, 'no_open': True, 'no_edit':True}</attribute>
                <attribute name="readonly">stock_picking_type == 'exchange'</attribute>
                <attribute name="force_save">1</attribute>
            </xpath>
            <xpath expr="//field[@name='origin']" position="after">
                <field name="nhcl_replication_status" invisible="1" readonly="1" force_save="1"/>
                 <field name="scan_or_import" widget="radio"  options="{'horizontal': True}" invisible="stock_picking_type != 'receipt' or state == 'done'"/>
                 <field name="verify_barcode" invisible="stock_picking_type != 'receipt' or (scan_or_import == 'import' or state == 'done')"
                       style="border: 4px solid #714B67;padding-top: 5px;margin-bottom: 8px;"
                       readonly="state == 'done'"/>
                <field name="verification_success" invisible="stock_picking_type != 'receipt'" widget="badge" decoration-success="verification_success == 'matched'"/>
                <field name="store_name" invisible="stock_picking_type != 'exchange' or company_type != 'other'"
                       required="company_type == 'other'" readonly="state == 'done'" options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                <field name="store_pos_order" invisible="stock_picking_type != 'exchange' or company_type != 'other'"
                       readonly="state == 'done'" required="company_type == 'other'"/>
                <button name="nhcl_get_pos_order_from_diff_store" type="object" string="Get Data"
                        invisible="store_pos_order == False" class="oe_highlight"/>
            </xpath>
            <!--            <xpath expr="//field[@name='origin']" position="after">-->
            <!--                <field name="nhcl_last_serial_number" readonly="1" force_save="1"-->
            <!--                       invisible="picking_type_code != 'incoming'"/>-->
            <!--                <field name="label_click_count"/>-->
            <!--            </xpath>-->
            <xpath expr="//button[@name='action_open_label_type']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//button[@class='o_btn_validate']" position="attributes">
                 <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='verification_success']" position="after">
                <button name="action_import_excel" type="object" string="Import Excel" class="btn-primary" invisible="stock_picking_type != 'receipt' or (scan_or_import == 'scan' or state == 'done')"/>
            </xpath>
<!--            <xpath expr="//button[@name='button_validate'][1]" position="attributes">-->
<!--                 <attribute name="invisible">is_picking_type == False</attribute>-->
<!--            </xpath>-->
            <xpath expr="//form/sheet/group/group/field[@name='picking_type_id']" position="before">
                <field name="company_type" invisible="stock_picking_type != 'exchange'" required="stock_picking_type == 'exchange'" readonly="state == 'done'"/>
                <field name="nhcl_pos_order" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       invisible="stock_picking_type != 'exchange' or company_type != 'same'"
                       readonly="state == 'done'"/>
                <field name="nhcl_purchased_store" invisible="stock_picking_type != 'exchange'" readonly="1" force_save="1"/>
                <field name="nhcl_invoice_date" invisible="stock_picking_type != 'exchange'" readonly="1" force_save="1"/>
                <field name="ref_credit_note" invisible="stock_picking_type != 'exchange'" readonly="1" force_save="1" options="{'no_create': True, 'no_edit':True}"/>
                <field name="currency_id" invisible="1"/>
                <field name="stock_picking_type" invisible="1"/>
<!--                <field name="is_picking_type" invisible="1"/>-->
                <field name="nhcl_credit_note_count" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']/tree//field[@name='product_uom_qty']"
                   position="after">
                <field name="type_product" column_invisible="parent.picking_type_code != 'incoming'"/>
                <field name="dummy_lot_ids" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       widget="many2many_tags" readonly="1" force_save="1" optional="hide"/>
                <field name="nhcl_exchange" column_invisible="parent.stock_picking_type != 'exchange'" required="parent.stock_picking_type == 'exchange'" readonly="parent.state == 'done' or type_product == False"/>
                <field name="nhcl_rsp" column_invisible="parent.stock_picking_type != 'exchange'" readonly="1" force_save="1"/>
                <field name="nhcl_tax_ids" widget="many2many_tags" column_invisible="parent.stock_picking_type != 'exchange'" readonly="1" force_save="1"/>
                <field name="nhcl_discount" column_invisible="parent.stock_picking_type != 'exchange'" readonly="1" force_save="1"/>
                <field name="nhcl_gdiscount" column_invisible="parent.stock_picking_type != 'exchange'" readonly="1" force_save="1"/>
                <field name="nhcl_disc_lines" column_invisible="parent.stock_picking_type != 'exchange'" readonly="1" force_save="1" optional="hide"/>
                <field name="nhcl_price_subtotal" column_invisible="parent.stock_picking_type != 'exchange'" readonly="1" force_save="1"/>
                <field name="currency_id" column_invisible="1"/>
                <field name="pos_order_lines" column_invisible="1"/>
                <field name="ref_pos_order_line_id" column_invisible="1"/>
                <field name="serial_no" column_invisible="parent.stock_picking_type != 'exchange' and parent.company_type != 'other'"/>
            </xpath>
            <xpath expr="//form/header" position="inside">
                <button name="reset_product_lines" type="object" string="Reset Lines" invisible="state == 'done'"/>
                <button name="stock_create_credit_note" type="object" class="oe_highlight" string="Credit Note" invisible="state != 'done' or stock_picking_type != 'exchange'"/>
                <button name="action_my_button" type="object" string="Credit Note Report" class="btn btn-secondary" invisible="stock_picking_type != 'exchange'"/>
            </xpath>
            <xpath expr="//form/sheet/group/group" position="inside">
                <field name="stock_operation_type" invisible="stock_picking_type != 'regular'"
                       readonly="state == 'done'"/>
                <field name="lot_qty" readonly="state == 'done'" invisible="stock_operation_type != 'scan' or stock_picking_type in ['receipt','exchange']"/>
                <field name="stock_barcode" readonly="state == 'done'" invisible="stock_operation_type != 'scan' or stock_picking_type in ['receipt','exchange']"
                       style="border: 4px solid #714B67;padding-top: 6px;margin-bottom: 10px;"
                       placeholder="Please, Scan Barcode Here......"/>
                <label for="stock_document" string="Document"/>
                <div>
                    <field name="stock_document" placeholder="Please Select Receipt..." class="oe_inline"
                           invisible="stock_operation_type != 'document'" readonly="state == 'done'"
                           options="{'no_create': True, 'no_open': True, 'no_edit':True}"/>
                    <button name="get_receipt_picking_lines" type="object" string="Get Lines"
                            class="oe_inline btn-primary" invisible="stock_operation_type != 'document'"/>
                </div>
            </xpath>
            <xpath expr="//form/sheet/group/group/field[@name='location_dest_id']" position="after">
                <field name="stock_type" placeholder="Please Stock Type..." readonly="state == 'done'"
                       />
                <field name="dummy_stock_type" placeholder="Please Stock Type..." invisible="1"/>
            </xpath>
<!--            <xpath expr="//field[@name='move_ids_without_package']" position="attributes">-->
<!--                <attribute name="readonly">stock_picking_type == 'exchange'</attribute>-->
<!--            </xpath>-->
            <xpath expr="//notebook" position="inside">
                <page name="verification" string="Verification" invisible="stock_type not in ['inter_state','intra_state']">
                    <field name="stock_verification_ids" >
                        <tree editable="bottom" create="0">
                            <field name="stock_product_id" options="{'no_create': True, 'no_open': True, 'no_edit':True}" readonly="1" force_save="1"/>
                            <field name="stock_serial" readonly="1" force_save="1"/>
                            <field name="stock_qty" readonly="parent.state == 'done'" force_save="1" sum="Quantity"/>
                        </tree>
                    </field>
                </page>
            </xpath>
            <xpath expr="//form/sheet/notebook/page[@name='operations']" position="inside">
                <group invisible="stock_picking_type != 'exchange'">
                    <group class="oe_subtotal_footer oe_right">
                        <field name="nhcl_tax_totals_json" widget="account-tax-totals-field" nolabel="1" colspan="2"
                               readonly="1"/>
                    </group>
                </group>
            </xpath>
            <xpath expr="//form" position="attributes">
                <attribute name="delete">false</attribute>
            </xpath>
        </field>
    </record>

    <record id="stock_vpicktree" model="ir.ui.view">
        <field name="name">view.picking.tree.cmr</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="delete">0</attribute>
            </xpath>
        </field>
    </record>

    <record id="nhcl_view_picking_type_form" model="ir.ui.view">
        <field name="name">view.picking.form.inherit</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sequence_code']" position="after">
                <field name="stock_picking_type" required="1"/>
            </xpath>
        </field>
    </record>

