                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="categ_5" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="categ_6" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="categ_7" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       readonly="1" force_save="1" optional="hide" />
                 <field name="categ_8" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       readonly="1" force_save="1" optional="hide" />
                <field name="descrip_1" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="descrip_2" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="descrip_3" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="descrip_4" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="descrip_5" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="descrip_6" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="descrip_7" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="descrip_8" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
            </xpath>
        </field>
    </record>

    <record id="your_module_stock_move_line_tree_readonly" model="ir.ui.view">
        <field name="name">stock.move.line.tree.readonly</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="delete">false</attribute>
                <attribute name="create">false</attribute>
            </xpath>
            <xpath expr="//field[@name='lot_name']" position="attributes">
                <attribute name="readonly">1</attribute>
                <attribute name="force_save">1</attribute>
            </xpath>
        </field>
    </record>

    <record id="view_move_line_tree_sum" model="ir.ui.view">
        <field name="name">stock.move.line.sum</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock_picking_batch.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quantity']" position="attributes">
                <attribute name="sum">Quantity</attribute>
            </xpath>
            <xpath expr="//tree" position="attributes">
                <attribute name="delete">false</attribute>
                <attribute name="create">false</attribute>
            </xpath>
        </field>
    </record>

    <record id="view_picking_move_tree_inherited_sum" model="ir.ui.view">
