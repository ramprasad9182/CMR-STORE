                <field name="category_8" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="description_1" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="description_2" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
                <field name="description_3" options="{'no_create': True, 'no_open': True, 'no_edit':True}"
                       placeholder="Please Select ...." optional="hide" readonly="1" force_save="1"/>
