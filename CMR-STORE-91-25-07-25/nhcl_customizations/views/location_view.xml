<odoo>


	<record id="dev_location_tree_view" model="ir.ui.view">
		<field name="name">dev.location.tree.view</field>
		<field name="model">dev.location.location</field>
		<field name="arch" type="xml">
		    <tree string="Transport Location" editable="bottom">
                <field name="name" required="1" />
 		    </tree>
		</field>
	</record>
	

	<!--Action-->
	<record id="action_dev_location" model="ir.actions.act_window">
        <field name="name">Transport Locations</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">dev.location.location</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="dev_location_tree_view"/>
		<field name="context">{'create': False,'edit':False}</field>
    </record>
    
    

 	<menuitem name="Transport Locations"
              id='menu_action_transport_location'
              parent="menu_transport_config"
              action="action_dev_location"
              sequence="1"/>
              
</odoo>
