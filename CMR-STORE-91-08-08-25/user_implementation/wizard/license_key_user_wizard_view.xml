<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_license_key_user_create_wizard_form" model="ir.ui.view">
        <field name="name">license.key.user.create.wizard.form</field>
        <field name="model">license.key.user.create.wizard</field>
        <field name="arch" type="xml">
            <form string="User Creation Confirmation">
                <div class="o_form_label">
                    Licenses Key Verification is Successful.Do You Want to Import Users in License Line Tab using License Key?
                </div>
                <footer>
                    <button name="action_create_users" string="Yes,Import" type="object" class="btn-primary"/>
                    <button name="action_cancel" string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="view_license_key_create_user_wizard_form" model="ir.ui.view">
        <field name="name">license.key.create.users.wizard.form</field>
        <field name="model">license.key.create.users.wizard</field>
        <field name="arch" type="xml">
            <form string="Users and Employees Creation Confirmation">
                <div class="o_form_label">
                    Do You Want to Create the Users using License Key?
                </div>
                <footer>
                    <button name="action_create_users_and_employees" string="Yes, Create" type="object" class="btn-primary"/>
                    <button name="action_cancelled" string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>