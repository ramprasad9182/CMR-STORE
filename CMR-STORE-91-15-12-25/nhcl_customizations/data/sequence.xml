<odoo>
        
        <record id="dev_stock_picking_tracking" model="ir.sequence">
            <field name="name">Delivery Order Tracking</field>
            <field name="code">stock.picking.tracking</field>
            <field name="prefix">TRA/</field>
            <field name="padding">4</field>
        </record>

         <record id="return_seq_picking_batch" model="ir.sequence">
         <field name="name">Batch Transfer</field>
         <field name="code">picking.batch.return</field>
         <field name="prefix">BATCH/RETURN/</field>
         <field name="padding">5</field>
         <field name="company_id" eval="False"/>
     </record>


    <record id="nhcl_product_product_round_up" model="product.product">
        <field name="name">Rounding Up</field>
        <field name="categ_id" ref="product.product_category_all"/>
        <field name="detailed_type">service</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">False</field>
        <field name="invoice_policy">order</field>
    </record>

    <!-- Round Down Product -->
    <record id="nhcl_product_product_round_down" model="product.product">
        <field name="name">Rounding Down</field>
        <field name="categ_id" ref="product.product_category_all"/>
        <field name="detailed_type">service</field>
        <field name="sale_ok">True</field>
        <field name="purchase_ok">False</field>
        <field name="invoice_policy">order</field>
    </record>
        
	
</odoo>
